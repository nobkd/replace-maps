{
    "name": "replace-maps",
    "description": "Replace Google Maps iframes with OSM",
    "author": "nobkd",
    "license": "Unlicense",
    "version": "1.0.0",
    "type": "module",
    "homepage": "https://github.com/nobkd/replace-maps#readme",
    "bugs": {
        "url": "https://github.com/nobkd/replace-maps/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nobkd/replace-maps.git"
    },
    "scripts": {
        "icons": "svgexport icons/plain.svg public/icons/48.png 48:48 && svgexport icons/plain.svg public/icons/96.png 96:96",
        "format": "prettier . --write",
        "build": "tsc && vite build",
        "watch": "vite build --watch --mode development --minify false",
        "dev": "vite",
        "serve:firefox": "web-ext run -s .dist",
        "serve:chrome": "web-ext run -t chromium -s .dist",
        "bundle": "nr build && web-ext build -s .dist -a .out"
    },
    "devDependencies": {
        "@antfu/ni": "^0.21.2",
        "@types/leaflet": "^1.9.3",
        "@types/webextension-polyfill": "^0.10.0",
        "prettier": "^2.8.7",
        "svgexport": "^0.4.2",
        "typescript": "^5.0.2",
        "vite": "^4.2.1",
        "vite-plugin-static-copy": "^0.13.1",
        "web-ext": "^7.6.0",
        "webextension-polyfill": "^0.10.0"
    },
    "dependencies": {
        "leaflet": "^1.9.3"
    }
}
