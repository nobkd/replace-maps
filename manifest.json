{
    "description": "Replace Google Maps iframes with OSM",
    "manifest_version": 2,
    "name": "Replace Maps",
    "version": "1.0",
    "homepage_url": "https://github.com/nobkd/replace-maps",
    "icons": {
        "48": "icons/48.png",
        "96": "icons/96.png",
        "128": "icons/128.png"
    },
    "permissions": [
        "webNavigation",
        "webRequest",
        "webRequestBlocking",
        "http://*/maps/embed*",
        "https://*/maps/embed*",
        "http://*/maps*output=embed*",
        "https://*/maps*output=embed*"
    ],
    "background": {
        "scripts": [
            "replace.js"
        ]
    }
}